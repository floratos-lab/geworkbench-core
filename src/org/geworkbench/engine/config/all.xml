		<gui-area name="SelectionArea"/>
	</plugin>
	<plugin id="sequencePanel" name="Sequence Panel" class="org.geworkbench.components.sequences.SequenceViewAppComponent">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="promoterPanel" name="Promoter Panel" class="org.geworkbench.components.promoter.PromoterView">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="SequenceRetriever" name="Sequence Retriever" class="org.geworkbench.components.sequenceretriever.SequenceRetriever">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="positionHistogram" name="Position Histogram" class="org.geworkbench.components.poshistogram.PositionHistogramAppComponent">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="dotmatrixPanel" name="Dot Matrix Panel" class="org.geworkbench.components.alignment.synteny.DotMatrixViewAppComponent">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="patternPanel" name="Patterns" class="org.geworkbench.components.patterns.PatternViewAppComponent">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="syntenyParameters" name="Synteny" class="org.geworkbench.components.alignment.panels.SyntenyAppComponent">
		<gui-area name="SelectionArea"/>
	</plugin>
	<!--
	<plugin id="genePanel" name="Gene Panel" class="org.geworkbench.components.selectors.GenericMarkerSelectorPanel">
		<gui-area name="SelectionArea"/>
	</plugin>
    -->
	<!-- New Gene Panel implementation -->
	<plugin id="genePanel" name="Gene Panel" class="org.geworkbench.components.selectors.GenePanel">
		<gui-area name="SelectionArea"/>
	</plugin>
	<plugin id="hierarchicalClusteringPanel" name="Dendrogram" class="org.geworkbench.components.hierarchicalclustering.HierClusterViewAppComponent">
		<gui-area name="VisualArea"/>
	</plugin>
