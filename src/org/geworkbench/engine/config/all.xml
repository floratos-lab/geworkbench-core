	</plugin>
	<plugin id="EVDPanel" name="Expression Value Distribution" class="org.geworkbench.components.microarrays.EVDPanel">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="scatterPlot" name="Scatter Plot" class="org.geworkbench.components.plots.ScatterPlot">
		<gui-area name="VisualArea"/>
	</plugin>
