	<plugin id="MicroarrayCenteringNormalizer" name="Microarray Centering Normalizer" class="org.geworkbench.components.normalization.MicroarrayCenteringNormalizer" source="normalization"/>
	<plugin id="missingValueNorm" name="Missing Values" class="org.geworkbench.components.normalization.MissingValueNormalizer" source="normalization"/>
	<plugin id="missingValuesFilter" name="Missing Values Filter" class="org.geworkbench.components.filtering.MissingValuesFilter" source="filtering"/>
	<plugin id="sequenceFileFilter" name="FASTA Format" class="org.geworkbench.components.parsers.sequences.SequenceFileFormat"/>
	<plugin id="scatterPlot" name="Scatter Plot" class="org.geworkbench.components.plots.ScatterPlot" source="plots">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="volcanoPlot" name="Volcano Plot" class="org.geworkbench.components.plots.VolcanoPlot" source="plots">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="somClusteringPanel" name="SOM Clusters" class="org.geworkbench.components.plots.SOMDisplay" source="plots">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="tabularMicroarrayPanel" name="Tabular Microarray Viewer" class="org.geworkbench.components.microarrays.TabularMicroarrayPanel" source="microarrays">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="tabularDataViewer" name="Tabular Viewer" class="org.geworkbench.components.anova.gui.TabularDataViewer" source="anova">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="phenotypePanel2" name="Arrays/Phenotypes" class="org.geworkbench.components.selectors.PhenotypePanel" source="selectors">
		<gui-area name="SelectionArea"/>
	</plugin>
	<plugin id="anova" name="Anova Analysis" class="org.geworkbench.components.anova.AnovaAnalysis" source="anova"/>

	<plugin id="aracne" name="ARACNE Analysis" class="org.geworkbench.components.aracne.AracneAnalysis" source="aracne-java"/>
	<plugin id="mindy" name="MINDY Analysis" class="org.geworkbench.components.mindy.MindyAnalysis" source="mindy"/>
	<plugin id="fastHierarchicalClusteringAnalysis" name="Hierachical Clustering" class="org.geworkbench.components.hierarchicalclustering.FastHierClustAnalysis" source="hierarchicalclustering"/>
	<plugin id="matrixReduce" name="Matrix Reduce" class="org.geworkbench.components.matrixreduce.MatrixReduceAnalysis" source="matrixreduce"/>
	<plugin id="matrixReduceViewer" name="MatrixREDUCE" class="org.geworkbench.components.matrixreduce.MatrixReduceViewer" source="matrixreduce">
		<gui-area name="VisualArea"/>
	</plugin>
	
	<plugin id="knnAnalysis_30" name="KNN Analysis_3.0" class="org.geworkbench.components.gpmodule.classification.knn.KNNTraining" source="gpmodule_v3_0"/>
	<plugin id="PCAAnalysis_30" name="PCA Analysis 3.0" class="org.geworkbench.components.gpmodule.pca.PCAAnalysis" source="gpmodule_v3_0"/>
	<plugin id="somAnalysis" name="SOM Analysis" class="org.geworkbench.components.somclustering.SOMAnalysis" source="somclustering"/>
	
	<plugin id="PCAViewer_30" name="PCA Viewer 3.0" class="org.geworkbench.components.gpmodule.pca.PCA" source="gpmodule_v3_0">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="tTestAnalysis" name="t Test Analysis" class="org.geworkbench.components.analysis.clustering.TtestAnalysis" source="analysis"/>
	<plugin id="wvAnalysis_30" name="WV Analysis 3.0" class="org.geworkbench.components.gpmodule.classification.wv.WVTraining" source="gpmodule_v3_0"/>
	
	<plugin id="thresholdNormalizer" name="Threshold Normalizer" class="org.geworkbench.components.normalization.ThresholdNormalizer" source="normalization"/>
	<plugin id="QuantileNormalizer" name="Quantile Normalizer" class="org.geworkbench.components.normalization.QuantileNormalizer" source="normalization"/>
	<plugin id="TwoChannelFilter" name="Genepix Filter" class="org.geworkbench.components.filtering.GenepixExpressionThresholdFilter" source="filtering"/>
	<plugin id="preferences" name="preferences" class="org.geworkbench.engine.config.PreferencesMenu"/>
	<plugin id="versioninfo" name="Version Infomation" class="org.geworkbench.components.versioninfo.VersionInfoComponent" source="versioninfo"/>
	<plugin id="promoterPanel" name="Promoter" class="org.geworkbench.components.promoter.PromoterView" source="promoter">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="sequencePanel" name="Sequence" class="org.geworkbench.components.sequences.SequenceViewAppComponent" source="sequences">
		<gui-area name="VisualArea"/>
	</plugin>

	<plugin id="CellularNetWork" name="Cellular NetWork" class="org.geworkbench.components.interactions.cellularnetwork.CellularNetworkKnowledgeWidget" source="interactions">
		<gui-area name="VisualArea"/>
	</plugin>

	<plugin id="SequenceRetriever" name="Sequence Retriever" class="org.geworkbench.components.sequenceretriever.SequenceRetriever" source="sequenceretriever">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="positionHistogram" name="Position Histogram" class="org.geworkbench.components.poshistogram.PositionHistogramAppComponent" source="poshistogram">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="patternPanel" name="Patterns" class="org.geworkbench.components.patterns.PatternViewAppComponent" source="patterns">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="microarrayPanel" name="Microarray Viewer" class="org.geworkbench.components.microarrays.MicroarrayPanel" source="microarrays">
		<gui-area name="VisualArea"/>
		<!-- An example 'subscription' directive: Set enabled to 'No' to prevent subscription for certain type. -->
		<subscription type="org.geworkbench.events.ProjectEvent" enabled="Yes"/>
	</plugin>
	<plugin id="analysisPanel" name="Analysis" class="org.geworkbench.components.analysis.AnalysisPanel" source="analysis">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="commentsPanel" name="Dataset Annotation" class="org.geworkbench.builtin.projects.comments.CommentsPanel">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="filteringPanel" name="Filtering" class="org.geworkbench.components.filtering.FilteringPanel" source="filtering">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="historyPanel" name="Dataset History" class="org.geworkbench.builtin.projects.history.HistoryPanel">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="infoPanel" name="Experiment Info" class="org.geworkbench.builtin.projects.experimentinformation.ExperimentInformationPanel">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="sequenceAlignment" name="Sequence Alignment" class="org.geworkbench.components.alignment.panels.BlastAppComponent" source="alignment">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="splash" name="Pattern Discovery" class="org.geworkbench.components.discovery.SequenceDiscoveryViewAppComponent" source="discovery">
		<gui-area name="CommandArea"/>
	</plugin>
	<plugin id="normalizationPanel" name="Normalization" class="org.geworkbench.components.analysis.NormalizationPanel" source="analysis">
		<gui-area name="CommandArea"/>
	</plugin>

	<!-- MarkUs -->
    <plugin id="MarkUsAnalysis" name="MarkUs Analysis" class="org.geworkbench.components.markus.MarkUsAnalysis" source="markus"/>
    <plugin id="MarkUsBrowser" name="MarkUs Browser" class="org.geworkbench.components.markus.MarkUsBrowser" source="markus">
    <gui-area name="VisualArea"/>
    </plugin>

	<!-- MINDY -->
	<plugin id="mindyViewer" name="MINDY Viewer" class="org.geworkbench.components.mindy.MindyVisualComponent" source="mindy">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="Jmol" name="Jmol" class="org.geworkbench.components.jmol.JmolComponent" source="jmol">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="CELImageViewer" name="CELImageViewer" class="org.geworkbench.components.celprocessing.CELImageViewerComponent" source="celprocessing">
		<gui-area name="VisualArea"/>
	</plugin>
	<plugin id="caArray2Panel" name="CaArray2" class="org.geworkbench.components.caarray.arraydata.CaArray2Component" source="caarray">
    </plugin>
    
	<plugin id="genspaceLogPreferences" name="genSpace Log Preferences"
		class="org.geworkbench.components.genspace.GenSpaceLogPreferences"
		source="genspace">
		<gui-area name="CommandArea" />
	</plugin>
	<plugin id="genspaceLogger" name="Genspace"
		class="org.geworkbench.components.genspace.GenspaceLogger"
		source="genspace">
	</plugin>
	
