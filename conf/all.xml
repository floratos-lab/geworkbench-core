		class="org.geworkbench.components.parsers.sequences.SequenceFileFormat" />

	<plugin id="genepixFilter" name="Genepix File Format"
		class="org.geworkbench.components.parsers.GenePixFileFormat" />
	
	<plugin id="rmaExpressFileFilter" name="RMA Express Format"
		class="org.geworkbench.components.parsers.RMAExpressFileFormat" />
		
	<!-- 
	<plugin id="edgeListFilter" name="Edge List File Format"
		class="org.geworkbench.components.parsers.EdgeListFileFormat" />
	
	 -->	

	<plugin id="commentsPanel" name="Dataset Annotation"
		class="org.geworkbench.builtin.projects.comments.CommentsPanel">
		<gui-area name="CommandArea" />
	</plugin>
	<plugin id="historyPanel" name="Dataset History"
		class="org.geworkbench.builtin.projects.history.HistoryPanel">
		<gui-area name="CommandArea" />
	</plugin>
	<plugin id="infoPanel" name="Experiment Info"
		class="org.geworkbench.builtin.projects.experimentinformation.ExperimentInformationPanel">
		<gui-area name="CommandArea" />
	</plugin>

</geaw-config>