<?xml version="1.0" encoding="UTF-8"?>
<geaw-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ConfigurationFile Schema.xsd">

	<gui-window name="Main GUI" class="org.geworkbench.engine.skin.Skin" />
    
    <!-- Activate it to make SOFT parser Working -->   
    <!-- <plugin id="SoftFileFilter" name="soft. Format"
		class="org.geworkbench.components.parsers.SOFTFileFormat" /> -->

	<plugin id="expressionFileFilter" name="Exp. Format"
		class="org.geworkbench.components.parsers.ExpressionFileFormat" />

	<plugin id="pdbFileFilter" name="PDB Structure Format"
		class="org.geworkbench.components.parsers.PDBFileFormat" />

	<plugin id="projectWindow" name="Project Panel"
		class="org.geworkbench.builtin.projects.ProjectPanel">
		<gui-area name="ProjectArea" />
	</plugin>

	<plugin id="caarrayPanel" name="CaArray Panel"
		class="org.geworkbench.builtin.projects.util.CaARRAYPanel">
	</plugin>

	<plugin id="configurationMenu" name="configurationMenu"
		class="org.geworkbench.engine.ccm.ComponentConfigurationManagerMenu" />
	<plugin id="preferences" name="preferences"
		class="org.geworkbench.engine.config.PreferencesMenu" />

	<plugin id="CELLoader" name="CEL File Loader"
		class="org.geworkbench.components.parsers.CELFileFormat" />

	<plugin id="affyFilter" name="Affy File Format"
