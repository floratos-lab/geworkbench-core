<?xml version="1.0" encoding="UTF-8"?>
<geaw-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ConfigurationFile Schema.xsd">

	<gui-window name="Main GUI" class="org.geworkbench.engine.skin.Skin" />
	<!-- Preferences is only loaded here so that its help file appears -->
	<plugin id="Preferences" name="for Preferences Help"
		class="org.geworkbench.engine.preferences.Preferences" />	
	
	<!-- FileFormatHelp is a dummy class used only to load the help file. -->
	<plugin id="fileFormatHelp" name="for File Format Help"
		class="org.geworkbench.parsers.FileFormatHelp" />

	<!-- LocalDataFilesHelp is a dummy class used only to load the help file-->	
	<plugin id="localDataFilesHelp" name="for Local Data File Help"
		class="org.geworkbench.builtin.projects.LocalDataFilesHelp" />	

	<plugin id="SoftFileFilter" name="soft. Format"
		class="org.geworkbench.parsers.SOFTFileFormat" />

	<plugin id="MageTabFileFilter" name="MAGE-TAB. Format"
		class="org.geworkbench.parsers.MageTabFileFormat" />

	<plugin id="expressionFileFilter" name="Exp. Format"
		class="org.geworkbench.parsers.ExpressionFileFormat" />

	<plugin id="pdbFileFilter" name="PDB Structure Format"
		class="org.geworkbench.parsers.PDBFileFormat" />

	<plugin id="projectWindow" name="Project Panel"
		class="org.geworkbench.builtin.projects.ProjectPanel">
		<gui-area name="ProjectArea" />
	</plugin>

	<plugin id="caarrayPanel" name="CaArray Panel"
		class="org.geworkbench.builtin.projects.util.CaARRAYPanel">
	</plugin>

	<plugin id="WelcomeScreen" name="Welcome Screen"
		class="org.geworkbench.engine.WelcomeScreen">
		<gui-area name="VisualArea" />
	</plugin>

	<plugin id="configurationMenu" name="configurationMenu"
		class="org.geworkbench.engine.ccm.ComponentConfigurationManagerMenu" />
	<plugin id="preferences" name="preferences"
		class="org.geworkbench.engine.config.PreferencesMenu" />

	<plugin id="shortcuts" name="shortcuts"
		class="org.geworkbench.engine.skin.ShortCutMenu" />

	<plugin id="CELLoader" name="CEL File Loader"
		class="org.geworkbench.parsers.CELFileFormat" />

	<plugin id="affyFilter" name="Affy File Format"
		class="org.geworkbench.parsers.AffyFileFormat" />

	<plugin id="adjMatrixFilter" name="Addjacency Matrix File Format"
		class="org.geworkbench.parsers.AdjacencyMatrixFileFormat" />

	<plugin id="patFilter" name="Pattern File Format"
		class="org.geworkbench.parsers.PatternFileFormat" />
		
	<plugin id="sequenceFileFilter" name="FASTA Format"
		class="org.geworkbench.parsers.sequences.SequenceFileFormat" />

	<plugin id="genepixFilter" name="Genepix File Format"
		class="org.geworkbench.parsers.GenePixFileFormat" />

	<plugin id="tabDelimitedDataMatrixFileFilter" name="Tab-Delimited Data Matrix File Format"
		class="org.geworkbench.parsers.TabDelimitedDataMatrixFileFormat" />

	<!--
	<plugin id="edgeListFilter" name="Edge List File Format"
		class="org.geworkbench.components.parsers.EdgeListFileFormat" />

	 -->

	<plugin id="commentsPanel" name="Dataset Annotation"
		class="org.geworkbench.builtin.projects.comments.CommentsPanel">
	</plugin>
	<plugin id="historyPanel" name="Dataset History"
		class="org.geworkbench.builtin.projects.history.HistoryPanel">
	</plugin>
	<plugin id="infoPanel" name="Experiment Info"
		class="org.geworkbench.builtin.projects.experimentinformation.ExperimentInformationPanel">
	</plugin>

	<!-- this must be after the three panels above -->	
	<plugin id="informationPanel" name="Information Panel"
		class="org.geworkbench.builtin.projects.InformationPanel">
		<gui-area name="VisualArea" />
	</plugin>

</geaw-config>